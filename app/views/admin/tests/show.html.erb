<h1>Test <%= @test.title %></h1>
<div class="links">
  <%= link_to "Test list", admin_tests_path %>  |
  <%= link_to "Edit", edit_admin_test_path(@test) %>  | 
  <%= link_to "Create test", new_admin_test_path(@test) %>
</div>

<p>Category: <%= @test.category.title %></p>
<p>Level: <%= @test.level %></p>

<h2>Question to test</h2>
<div style="margin: 10px;"><%= link_to "Add new Question to this test", new_admin_test_question_path(@test)%></div>


<table style="margin: 10px;">

  <thead>
    <th>Text of Questions</th>
    <th colspan='3'>Action</th>
  </thead>
  <tbody>
    <%# <%= render @test.questions %> 
    <%= render partial: 'admin/questions/question', collection: @test.questions%>
  </tbody>
</table>