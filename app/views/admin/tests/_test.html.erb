<tr>
  <td>
     <%= link_to test.title, 
                admin_test_path(test), 
                class: "test-title", 
                data: { test_id: test.id } %>
      <%= render 'form_inline', test: test = @test ? @test : test %>              
  </td>
  <td> <%= test.level %> </td>
  <td> <%= test.category.title %> </td>
  <td> <%= test.questions.count %> </td>
  <td><%= link_to t('.show'), admin_test_path(test) %> </td>
	<td><%= link_to t('.edit'), edit_admin_test_path(test), data: { test_id: test.id }, class: "form-inline-link" %> </td>
  <td> <%= link_to t('.delete'), admin_test_path(test), method: :delete, data: { confirm: t('.message') } %> </td>
</tr>